.assetInput{padding:0 5vw 5vw;
  input{border:1px solid $lightest;height:10vw;text-align: center;}
}

.tradeMain{height:100vh;width:100vw;background: #121b31;color:$lighter;
  .tmHead{display:flex;justify-content:space-between;align-items:center; height:46px;
    .tmhLeft{display: flex;
      i{display:flex;justify-content:center;align-items:center; width:46px;height:46px;font-size:5.3333vw;}
      b{font-size:4.2667vw;line-height:46px;}
