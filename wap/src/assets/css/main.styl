.coinlistMini{
  li{
    display:flex;padding:3.5vw 3vw;border-top:1px solid $lightest;font-size:4.5vw;
    .clleft{width:30%;}
    .clcenter{width:40%;font-size:bold;font-size:4.2vw;}
