.van-picker__cancel:active, 
.van-picker__confirm:active{background:none;}

// 首页列表内容
.coinlist{
  padding: 0 4vw;
  li{display: flex;min-height: 10.2667vw;align-items:center;font-size:4.2vw;padding: 2.6667vw 0;
    .clleft{width:35vw;padding-right: 3vw;
      >p{
        display: flex;
        align-items: center;
        b{
          font-size: 4vw;
        }
        span{font-size:2.9333vw;}
        .op5{font-size:3.2vw;}
        .vol{margin-right: 1vw;}
      }
    }
    .clcenter{flex: 1;padding-right: 3vw;
      >p{
        display: flex;
        b{font-size:4vw;}
        .cur{font-size:2.9333vw;}
      }
    }
    .clright{
      .zdbtn{display: inline-flex;justify-content:center;align-items:center;font-size:3.4667vw;width:17.8667vw;height:7.7333vw;border-radius: 1vw;color:$lightBg;font-weight: 500;
        &.green{background: $green;}
        &.red{background: $red;}
        &.normal{background:$lighter;}
      }
    }
  }
}

//==============官方公告和新手指导===============
.articleList{
  padding: 0 4vw;
  li{
    &:first-child{padding-top:1.3333vw;}
    padding: 4vw 0;
    h3{font-size:4vw;position: relative;padding-left: 4vw;font-weight: normal;letter-spacing: 0.0667vw;font-weight: 500;
      .dot{width: 2vw;height: 2vw;border-radius: 50%;background: $blue;position: absolute;top:2vw;left:0;}
    }
    .articleContent{font-size: 3.2vw;margin: 1vw 0 2vw;line-height: 4.4vw;text-align: justify;}
    .date{font-size: 2.9333vw;margin-left: 4vw;}
  }
}
.article{
  padding:1.3333vw 4vw 0;display: flex;flex-direction: column;
  h1{font-size: 4.8vw;text-align: justify;}
  .date{font-size:2.9333vw;margin: 1.3333vw 0 4vw;}
  .content{font-size: 3.2vw;text-align: justify;padding-bottom: 8vw;}
}

.coinTitle{padding:3vw 3vw;font-size:4vw;display: flex;align-items:center;}
.coinTh{display: flex;padding:3vw 3vw;border-top:1px solid $lightest;border-bottom:1px solid $lightest;
  span{font-size:3.4vw;
    &:nth-child(1){width:45vw;}
    &:nth-child(2){width:30vw;text-align:right; }
    &:nth-child(3){width:18vw;text-align:right;}
  }
}

//===============行情侧拉框============
.drawPage{
  .van-popup{height:100vh;width: 57.3333vw;overflow: hidden;} 
}
.drawHead{font-size:4.2667vw;padding: 2.9333vw;
  span{font-weight: bold;}
}
.drawContent{
  .drawUl{
    li{
      font-weight: 500;
      padding: 2.6667vw 4vw;
      justify-content:space-between;
      &:first-child{
        padding-top: 4.6667vw;
      }
      >b{
        font-size:3.7333vw;
        font-weight: normal;
      }
      >span{
        font-size: 2.9333vw;
        &:last-child{
          color: $green;
        }
      }
    }
  }
}


.detailListBox{padding:2vw 4vw;
  .title{display:flex;
    span{ display:flex;align-items:center;font-weight:bold;
      &:nth-child(1){width:38%;}
      &:nth-child(2){flex:1;}
      &:nth-child(3){}
    }
  }
}

// 行情侧边栏tab
.vanTab{
  .van-tabs__wrap{height: 9.0667vw;}
  .van-tabs__nav{height: 9.0667vw;display:flex;}
  .van-tab{
    flex-basis: 20% !important;
    line-height: 9.0667vw;
    font-size: 3.7333vw;
    opacity: 1;
    font-weight: 500;
    padding: 0;
  }
  .van-tabs__line{
    width: 3.7333vw !important;
    transform: translateX(4vw) translateX(-50%);
    margin-top: 0.5vw !important;
    bottom: 0;
    background:$blue;
  }
}


// 行情tradeListTitle
.tradeListTitle{
  padding: 2.6667vw 4vw 0;
  display: flex;
  align-items: center; 
  font-size: 2.9333vw;
  span{
    &:first-child{width:35vw;padding-right: 3vw;}
    &:nth-of-type(2){flex: 1;}
    &:last-child{text-align: right;}
  }
}


// OTC二级菜单
.otcB{
  padding: 0;
  .noLine{
    border-bottom: none;
    background: #fff;
    padding: 0 4vw;
  }
}
//交易侧拉框二级菜单
.quotesB{
  .menusHeader{
    .tab{
      max-width: 200vw; 
      overflow: hidden;
      li{
        &:first-child{margin-left: 0;}
      }
    }
    // .tab::-webkit-scrollbar{display: none;}
    .listTitle{
      padding: 2.6667vw 4vw 0;
    }
  }
}

//================行情================
.quotesHead{
  height: 11.7333vw; 
  font-size: 4.2667vw;
  font-weight: 500;
}
.coinlistHead{padding: 2.9333vw;
  .coinright{
    display: flex;align-items:center;
    .coindrawer{font-size: 4.2667vw;}
  }
  &.coinlistHeadMini{padding:0 2vw 0 2vw;
    .cointab{margin-right:2vw;}
    .coinright{
      .coindrawer{position:absolute;right:0;top:0;padding:0 4vw;}
    }
  }
  .cointab{margin-right:5vw; padding:0 10px;display: flex;height:46px;font-weight:bold;color:$light;font-size:4vw;align-items:center;
    &.active{color:$c;position: relative;
      &:after{content:"";background: $c;height:1vw;position: absolute;left:0;bottom:0;right:0;}
    }
  }
}

// 交易*****委托当前和历史成交
.chooseTradeType{height:12.8vw;box-sizing:border-box;position:relative;
  i{font-size: 5.3333vw;text-align:left;position:absolute;left: 4vw;top center;}
  span{font-size:4vw;border-radius: 1vw;color:$nineblack;line-height:7.7333vw;height: 7.7333vw;border:none;
    &.active{background:$blue;color:$lightBg;}
    &:first-child{margin-right: 3.6vw}
  }
  .trading{padding:0 4.6667vw;}
  .historyOrder{padding:0 2.1333vw;
    &:first-child{margin-right: 2vw}
  }
  &.mini{
    span{font-size:3.6vw;margin:0 1vw;}
  }
}


// 下拉菜单
.dropDownMenus{padding-top: 1.3333vw;
  .van-dropdown-menu{height:9.0667vw;border:none;
    .van-dropdown-menu__item{display: flex;align-items:center;}
    .van-dropdown-menu__title{font-size: 3.6vw;color:#A2AEBF;}
    .van-dropdown-menu__title::after {border: 4px solid;border-color: transparent transparent #A2AEBF #A2AEBF;}
    .van-dropdown-menu__title--down::after{border-color:transparent transparent $blue $blue;}
    .van-cell{
      &:last-child{border-bottom:none;}
    }
    .van-cell::after{border:none;}
  }
}

.setAll{float:right;}
.tradeDrawerTitle{
  font-size:4.2667vw;color:$threeblack;padding: 2.9333vw;
}
//交易详情界面
.tradeHeaderWrap{
  height:9.0667vw;
}
.assetsPop{
  .van-dialog__header{padding:5vw 0;line-height:100%;}
}
.tradeHeader{display:flex;justify-content:space-between;height:9.0667vw;padding: 0 4vw;
  &.fixed{position:fixed;left:0;right:0;top:0;background:$lightBg;z-index:50;}
  .thLeft{
    span{font-size:4.2667vw;font-weight: bold;}
    i{
      margin-right: 2vw;
    }
  }
  .thRight{ 
    i:nth-of-type(2){
      margin-left: 4vw;
    }
  }
  i{font-size:5.3333vw;}
}
.tradeBox{
  margin-bottom: 2.6667vw;
  .tradeLeft{
    .van-button{border:none;border-radius: 1vw;}
  }
}
// 交易和秒合约共用部分
.tradeTitle{
  padding: 4vw;line-height: 100%;display: flex;justify-content: space-between;align-items:center;
  b{font-size:4.2667vw;font-weight:normal}
  span{font-size:3.2vw;}
}

.tradeSlider{position: relative;padding-top:4vw;
  .vslider{
    .van-slider{z-index:22;}
  }
  .vsliderNum{display: flex;justify-content:space-between;color:$light;font-size:3.2vw;padding-top:3vw;}
  .customButton{display: flex;justify-content:center;align-items:center;width: 5vw; height: 5vw;background: rgba(0,0,0,0);
    span{width:2.4vw;height:2.4vw;border-radius: 50%;}  
  }
  .tradeSliderDownI{position: absolute;display: flex; justify-content:space-between;top:3.8vw;left:0;right:0;
    i{width:1.0667vw;height:1.0667vw;border-radius: 50%;}
  }
}

.secOrderPrice{font-weight:bold;font-size:6vw;}
.secOrderDes{font-size:3.2vw;}
.secChoose{padding:3vw 3vw 0 0;
  ul{background:#f7f8fa;
    li{display:flex;justify-content:space-between;align-items:center;border-top:1px solid #fff;padding:3vw 3vw;}
  }
}

.kline{text-align:center;min-height:30vh; display:flex;justify-content:center;align-items:center;}
.tradeBox{display: flex;padding:1.6vw 0 4vw 4vw;
  .selector{border:1px solid #ddd;height:7vw;display:flex;justify-content:center;align-items:center;width:47%;position:relative;box-sizing:border-box;padding-right:3vw;
    &:before{content:"";position:absolute;right:2vw;top:2.8vw; border-style:solid;border-width:1vw;border-color:#666 transparent transparent transparent;}
  }
  .tradeLeft{width:53.3333vw;box-sizing:border-box;
    &.typeSec{width:70%;}
    .tradeBtn{display:flex;padding:0vw;justify-content:space-between;color:$lighter;font-size:3.7333vw;line-height:100%;
     
      >div{width: 25.7333vw;height: 9.0667vw;border-radius:0.5333vw;font-size:3.7333vw;color:#A2AEBF;
        &.buying{background:url('../img/buy.png') no-repeat;background-size:100% 100%;}
        &.selling{background:url('../img/sell.png') no-repeat;background-size:100% 100%;}
        &.active.buying{color:#fff;background-image:url('../img/buy_active.png');}
        &:first-child{margin-right: 1.8667vw;}
        &.active.selling{color:#fff;background-image:url('../img/sell_active.png');}
      }
      

    }
    .pt2{
      .numberBox{padding: 0;box-sizing:border-box;
        input[type="number"]{height:10vw;font-size:3.2vw;}
        i{color:$nineblack;font-size:4vw;height:10vw}
      }
      .sumPrice{font-size: 2.9333vw;padding-top: 1.3333vw;
        b{font-size: 3.7333vw;padding: 0 1vw;}
      }
    }
    .pt3{
      .van-button{border-radius: 1vw;color:#fff;font-size:3.7333vw;}
      .buying{color:#fff;background: $green;}
      .selling{color:#fff;background: $red;}
    }
    .tradeSubmit{height:12vw;line-height:12vw;font-size:4vw;border-radius: 0.5333vw;}
    .bsTag{display: flex;align-items:center; justify-content:center;
      label{ display: flex;align-items:center;color:$light;
        i{width:2vw;height:2vw;margin-right:1vw;}
        &:first-child{margin-right:5vw;
          i{background: $green;}
        }
        &:last-child{
          i{background: $red;}
        }
      }
    }
  }
  .tradeRight{flex:1;box-sizing:border-box; margin-left:4vw;
    &.typeSec{width:30%;}
    .trThead{padding-right:4vw;
      span{ font-size:2.9333vw;color:$nineblack;}
    }
    .trList{color:$light;
      &.buy{
        span.dbg{background: $green; }
        span:nth-child(2){color:$green;}
      }
      &.sell{
        span.dbg{background: $red; }
        span:nth-child(2){color:$red;}
      }
      li{display: flex;padding-right:0vw; position: relative;
        span{position: relative;z-index:2;font-size:3.2vw;line-height:7.2vw;
          // &:nth-child(1){max-width:15px;}
          &:nth-child(1){overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
          &:nth-child(2){margin-right:4vw;flex:1;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;text-align: right;}
          &.dbg{position: absolute;z-index:1;right:0;top:0;bottom:0;opacity: 0.2;}
        }
      }
    }
    .trPrice{padding:2vw 0;
      h5{color:$black;font-size:5.6vw;line-height:100%;
        &.sellcolor{color:$red;}
        &.buycolor{color:$green;}
      }
      div{font-size:3.2vw;color:$light;padding-top:1vw;}
    }
  }
}


.chatme{
  position:relative;
  .unread{font-size:3vw; position:absolute;right:-3vw;top:0vw;}
}

