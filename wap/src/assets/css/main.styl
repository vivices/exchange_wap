@import("./base")
.whiteModel{
  @import("./white")
}

.darkModel{
  @import("./dark") 
}

body{font:14px/1.5 "Heiti SC","Helvetica","HelveticaNeue","Droidsansfallback","Droid Sans","PingFang SC",Arial,"Helvetica Neue","Lantinghei SC","Microsoft YaHei","HanHei SC","Open Sans","Hiragino Sans GB","微软雅黑",STHeiti,"WenQuanYi Micro Hei",SimSun,sans-serif;}

#app
  min-height 100vh
  background:#f5f5f5;
  // color $fontColor
  font-family 'Avenir', Helvetica, Arial, sans-serif
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

// 标准阴影样式
.shadow{
  box-shadow: 0 0.2667vw 0.5333vw rgba(0,0,0,0.5);
}

.tvbox{transform:scale(0);opacity:0;position:absolute;}

.green{color:$green;}
.red{color:$red;}

.fontcolor{color:$c;}
.buycolor{color:$green;}
.sellcolor{color:$red;}


//================首页================
.flexPage{display: flex;flex-direction:column;height:100vh;width:100vw;
  .flexHead{
    &.assets{background:$c;color:#fff;}
  }
  .flexContent{flex:1;overflow-y:auto;overflow-x:hidden;}
}

.addressing{word-break:break-all;}
.downToUp{
  border-top-left-radius: 1.6vw;
  border-top-right-radius: 1.6vw;
  max-height: 80vh;
  overflow: srcoll;
  .van-action-sheet__item{font-size: 3.7333vw;}
  .van-hairline--top::after{border: none;}
}

// 主轴(水平垂直局中)
.flexSpindle{
  display: flex;
  justify-content: center;
  align-items: center;
}
// 侧轴(水平垂直局中)
.flexSide{
  display: flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
}
// 主轴(水平两边垂直局中)
.flexBothSidesVertical{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
// 主轴(垂直局中)
.flexSpindleVertical{
  display: flex;
  align-items: center;
}

.dh{
  height:50px;
}
.indexSwipe{
  width: 92vw;
  height: 39.3333vw;
  overflow: hidden;
  border-radius: 1.1333vw;
  .van-swipe{
    width: 92vw;
    height: 39.3333vw;
    overflow: hidden;
    border-radius: 1.1333vw;
  }
}

// 清除downbar上部虚线
.van-hairline--top-bottom::after{border:none;}	
.downbar{
  i{margin-bottom: 1vw;font-size:20px;display:inline-block;}
}

// .seeMore{padding:5vw;color:$light;}
// ****首页****
.homePage{
  padding: 0 4vw;
  .logo{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 2.8vw 0;
    &.noLogin{
      justify-content:space-between;
      >span>a{color:$c;}
    }
    img{
      width: 20vw;
      height: 8.2667vw;
      margin: 1.7333vw 0;
      display: block;
    } 
  }
  
}



@keyframes shake{
  0%{transform:translateY(0vw);}
  25%{transform:translateY(0.666667vw);}
  50%{transform:translateY(0vw);}
  75%{transform:translateY(-0.666667vw);}
  100%{transform:translateY(0vw);}
}
.startPage{position: fixed;left:0;right:0;bottom:0;top:0;z-index:9999;background: #132031;
  display: flex;justify-content:center;align-items:center;
  >span{display:inline-flex;width:33.333333vw;height:13.733333vw;background:url('../img/start_bg.png') no-repeat center center;background-size:100% 100%; position:relative;
      >img{width:4.933333vw;height:5.6vw; position:absolute;left:2.666667vw;top:5.6vw;z-index:2;animation: shake 0.5s linear infinite; }
  }
}



.rightImg{ text-align:center;
  img{width:50vw;}
}

// 首页消息通知
.noticeBarWrap{height: 40px;position: relative;overflow: hidden;box-sizing: border-box;
  .noticeBar{ top:0;
    position: relative;
    .van-notice-bar{display: flex;align-items:center;padding: 0;
      .van-notice-bar__content{font-size: 3.2vw;}
    }
  }
}
// 首页数据展示
.quotation{
  ul{display:flex;padding: 2.6667vw 0 4vw;justify-content: space-around;align-items:center;
    >li{width: 33%;text-align:center;
      .symbol{font-size: 3.2vw;font-weight:bold;}
      .price{font-size: 4.5333vw;font-weight:bold;
        color:$light;
        &.sellcolor{color:$red}
        &.buycolor{color:$green}
      }
      .rate{font-weight:bold;font-size:3vw;}
      .cny{font-size: 2.6667vw;}
    }
  }
}
// 首页功能模块
.Menus{
  padding: 2.6667vw 4vw;
  .indexMenus{
    display: flex;justify-content:space-between;

  }
  .gridBtn{
    width:25%; text-align: center;padding:4vw 0;
		>i{display: inline-flex;justify-content:center;align-items:center;width:10.4vw;height:10.4vw;border-radius: 1000px;font-size:6.13333vw;
		}
		p{padding-top:2vw;font-size:2.9333vw;}
    &:nth-child(1){
      i{background: #5092FF;}
    }
    &:nth-child(2){
      i{background: #F27978;}
    }
    &:nth-child(3){
      i{background: #F6AC28;}
    }
    &:nth-child(4){
      i{background: #61BD4F;}
    }
    &:nth-child(5){
      i{background: #D26DEE;}
    }
  }
  .ad{
    width: 92vw;
    height: 19.2vw;
    margin-top: 2.2667vw;
    img{
      display: block;
      width: 100%;
    }
  }
} 


